{
  "name": "@examples/napi",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "browser": "vite",
    "build": "napi-raw build --platform --js index.cjs --dts index.d.cts",
    "test": "ava reset-cache && cross-env TS_NODE_PROJECT=./tsconfig.json node --import @oxc-node/core/register ../../node_modules/ava/entrypoints/cli.mjs",
    "test-js": "ava reset-cache && ava"
  },
  "napi": {
    "binaryName": "example",
    "wasm": {
      "initialMemory": 16384,
      "browser": {
        "fs": true,
        "buffer": true
      }
    },
    "dtsHeader": "type MaybePromise<T> = T | Promise<T>",
    "dtsHeaderFile": "./dts-header.d.ts",
    "targets": [
      "wasm32-wasip1-threads"
    ]
  },
  "devDependencies": {
    "@napi-rs/cli": "workspace:*",
    "@napi-rs/triples": "workspace:*",
    "@napi-rs/wasm-runtime": "workspace:*",
    "@oxc-node/core": "^0.0.35",
    "@testing-library/dom": "^10.4.1",
    "@types/lodash": "^4.17.20",
    "@vitest/browser": "^4.0.8",
    "@vitest/browser-playwright": "^4.0.8",
    "@vitest/ui": "^4.0.8",
    "ava": "^6.4.1",
    "buffer": "^6.0.3",
    "core-js": "^3.46.0",
    "cross-env": "10.1.0",
    "electron": "^39.1.1",
    "lodash": "^4.17.21",
    "playwright": "^1.56.1",
    "rxjs": "^7.8.2",
    "sinon": "^21.0.0",
    "vite": "^7.2.2",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vitest": "^4.0.8",
    "web-streams-polyfill": "^4.2.0",
    "webdriverio": "^9.20.0"
  },
  "ava": {
    "extensions": {
      "ts": "module",
      "cts": "commonjs",
      "cjs": true,
      "js": true
    },
    "files": [
      "__tests__/**/*.spec.{ts,cts,js,cjs,mjs}"
    ],
    "workerThreads": false,
    "cache": false,
    "timeout": "10m"
  },
  "dependencies": {
    "@emnapi/core": "^1.7.0"
  }
}
