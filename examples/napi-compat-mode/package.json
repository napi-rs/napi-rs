{
  "name": "@examples/compat-mode",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "napi-raw build --no-js --features latest",
    "test": "ava"
  },
  "devDependencies": {
    "@napi-rs/cli": "workspace:*",
    "ava": "^6.0.0",
    "sinon": "^17.0.1"
  },
  "ava": {
    "extensions": [
      "ts",
      "tsx"
    ],
    "require": [
      "ts-node/register/transpile-only"
    ],
    "files": [
      "__tests__/**/*.spec.ts"
    ],
    "environmentVariables": {
      "TS_NODE_PROJECT": "../tsconfig.json"
    },
    "workerThreads": false,
    "cache": false,
    "timeout": "5m"
  }
}
