{
  "name": "@napi-rs/triples",
  "version": "2.0.0-alpha.3",
  "description": "Rust target triples objects",
  "keywords": [
    "Rust",
    "cross-compile",
    "napi",
    "n-api",
    "node-rs",
    "napi-rs"
  ],
  "author": "LongYinan <lynweklm@gmail.com>",
  "homepage": "https://github.com/napi-rs/napi-rs/tree/main/triples#readme",
  "license": "MIT",
  "type": "module",
  "main": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "import": {
        "default": "./index.js",
        "types": "./index.d.ts"
      },
      "require": {
        "default": "./index.cjs",
        "types": "./index.d.ts"
      }
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/napi-rs/napi-rs.git"
  },
  "bugs": {
    "url": "https://github.com/napi-rs/napi-rs/issues"
  },
  "scripts": {
    "build": "node --loader ts-node/esm/transpile-only ./generate-triple-list.ts && prettier --write ./index.cjs"
  },
  "devDependencies": {
    "lodash-es": "^4.17.21",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
