{
  "name": "@napi-rs/triples",
  "version": "2.1.0",
  "description": "Rust target triples objects",
  "keywords": [
    "Rust",
    "cross-compile",
    "napi",
    "n-api",
    "node-rs",
    "napi-rs"
  ],
  "author": "LongYinan <lynweklm@gmail.com>",
  "homepage": "https://github.com/napi-rs/napi-rs/tree/main/triples#readme",
  "license": "MIT",
  "type": "module",
  "main": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "import": {
        "default": "./index.js",
        "types": "./index.d.ts"
      },
      "require": {
        "default": "./index.cjs",
        "types": "./index.d.ts"
      }
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "files": [
    "index.js",
    "index.cjs",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/napi-rs/napi-rs.git"
  },
  "bugs": {
    "url": "https://github.com/napi-rs/napi-rs/issues"
  },
  "scripts": {
    "build": "node --import @oxc-node/core/register ./generate-triple-list.ts && ../node_modules/.bin/prettier --write ./index.cjs"
  },
  "devDependencies": {
    "@napi-rs/cli": "workspace:*",
    "@oxc-node/core": "^0.0.32",
    "@types/node": "^24.3.1",
    "es-toolkit": "^1.39.10"
  }
}
